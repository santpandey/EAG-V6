{
  "manifest_version": 3,
  "name": "Blood Report Analyzer",
  "version": "1.0",
  "description": "Upload your blood report and ask questions about it.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  },
  "permissions": ["storage"],
  "host_permissions": [
    "http://localhost:8080/*"
  ]
}
